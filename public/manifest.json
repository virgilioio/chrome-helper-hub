{
  "manifest_version": 3,
  "name": "GoGio â€“ LinkedIn Companion",
  "description": "Add candidates from LinkedIn into GoGio pipelines in seconds.",
  "version": "0.1.0",
  "action": {
    "default_popup": "index.html",
    "default_title": "GoGio",
    "default_icon": {
      "16": "icon-128.png",
      "48": "icon-128.png",
      "128": "icon-128.png"
    }
  },
  "icons": {
    "16": "icon-128.png",
    "48": "icon-128.png",
    "128": "icon-128.png"
  },
  "permissions": [
    "storage",
    "tabs",
    "identity"
  ],
  "host_permissions": [
    "https://www.linkedin.com/*",
    "https://etrxjxstjfcozdjumfsj.supabase.co/*"
  ],
  "content_scripts": [
    {
      "matches": ["https://www.linkedin.com/in/*"],
      "js": ["linkedin-content.js"],
      "run_at": "document_idle"
    }
  ]
}
